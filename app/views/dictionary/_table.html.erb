<table class="table table-striped">
  <tr>
    <th>
      Field name
    </th>
    <th>
      What you get
    </th>
    <th>
      Value for <%= @applicant.identity.full_name %>
    </th>
  </tr>
  <% field_name_descriptions.each do |row| %>
    <%= render "field_row", name: row[0], what: row[1] %>
  <% end %>
</table>
